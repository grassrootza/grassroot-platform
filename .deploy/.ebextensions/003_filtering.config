"ListenerRule": {
  "Type": "AWS::ElasticLoadBalancingV2::ListenerRule",
  "Properties": {
    "Actions": [
      {
        "Type": "fixed-response",
        "FixedResponseConfig": {
          "ContentType": "text/html",
          "MessageBody": "<h1>Sorry not available</h1>",
          "StatusCode": "401"
        }
      }
    ],
    "Conditions": [
      {
        "Field": "path-pattern",
        "Values": [
          "/test"
        ]
      }
    ],
    "ListenerArn": {
      "Ref": "Listener"
    },
    "Priority": 1
  }
}